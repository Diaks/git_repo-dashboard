<!DOCTYPE html>
<html>
    <head>
       <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
        <title>{% block title %}DashBoard Tool{% endblock %}</title>
        
        {% block stylesheets %}
        
        	<link rel="stylesheet" href="{{ asset('bundles/template/css/body.css') }}" />
        	<link rel="stylesheet" href="{{ asset('bundles/template/css/local.css') }}" />
        	<link rel="stylesheet" href="{{ asset('bundles/template/css/exception.css') }}" />
        	<link rel="stylesheet" href="{{ asset('bundles/template/css/cake.generic.css') }}" />
        	<link rel="stylesheet" href="{{ asset('bundles/template/css/bootstrap.css') }}" />
        	<link rel="stylesheet" href="{{ asset('bundles/template/css/bootstrap-theme.min.css') }}" />
        	<link rel="stylesheet" href="{{ asset('bundles/template/css/jquery.gritter.css') }}" />
        	<link rel="stylesheet" href="{{ asset('bundles/template/css/style.css') }}" />
        	<link rel="stylesheet" href="{{ asset('bundles/template/css/style-responsive.css') }}" />
        	<link rel="stylesheet" href="{{ asset('bundles/template/css/jquery-ui.css') }}" />
        	<link rel="stylesheet" href="{{ asset('bundles/template/css/jquery-ui.structure.css') }}" />
        	<link rel="stylesheet" href="{{ asset('bundles/template/css/jquery-ui.theme.css') }}" />
        	<link rel="stylesheet" href="{{ asset('bundles/template/js/ckeditor/contents.css') }}" />
        	
        	
       		<link rel="stylesheet" href="{{ asset('bundles/template/css/custom.css') }}" />
        
		{% endblock %}
		
		<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
		<script src="{{ asset('bundles/template/js/jquery-ui-1.9/jquery-ui.min.js') }}"></script>
		
		
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
    	<div id="container">
    	
    		{% if app.session.get('type_profil') != null %}
    			<div id="header" class="header black-bg">
				 	{{ include('::layout/common/headerBar.html.twig') }}
				</div>
			{% endif %}
				
			<div id="content">
				{% if app.session.get('type_profil') != null %}
				<div id="header" class="header black-bg">
				 	{{ include('::layout/common/headerBar.html.twig') }}
				</div>
					{% if app.session.get('type_profil') == '1' %}
						{{ include('::layout/common/side_Analyst.html.twig') }}
					{% elseif  app.session.get('type_profil') == '2' %}
						{{ include('::layout/common/side_Admin.html.twig') }} 
					{% elseif  app.session.get('type_profil') == '10' %}
						{{ include('::layout/common/side_Price.html.twig') }}
					{% elseif  app.session.get('type_profil') == '11' %}
						{{ include('::layout/common/side_EDC.html.twig') }} 
					{% else %}
						{{ include('::layout/common/side_Analyst.html.twig') }}
					{% endif %}
				{% endif %}
				<div id="main">
					<section id="container">
						<section id = "main-content">
							<section class="wrapper">
        						{% block body %}
        
        						{% endblock %}
        					</section>
        				</section>
        			</section>
        		</div>
        	</div>
        </div>
        
        {% block javascripts %}
        <script type="text/javascript" src="{{ asset('bundles/template/js/bootstrap.min.js') }}"></script>
        <script type="text/javascript" src="{{ asset('bundles/template/js/jquery.dcjqaccordion.2.7.js') }}"></script>
        <script type="text/javascript" src="{{ asset('bundles/template/js/jquery.scrollTo.min.js') }}"></script>
        <script type="text/javascript" src="{{ asset('bundles/template/js/jquery.nicescroll.js') }}"></script>
        <script type="text/javascript" src="{{ asset('bundles/template/js/jquery.sparkline.js') }}"></script>
        
        
        <script type="text/javascript" src="{{ asset('bundles/template/js/jquery.simplemodal.1.4.4.min.js') }}"></script>
        
        <script type="text/javascript" src="{{ asset('bundles/template/js/ckeditor/ckeditor.js') }}"></script>
        <script type="text/javascript" src="{{ asset('bundles/template/js/ckeditor/build-config.js') }}"></script>
        <script type="text/javascript" src="{{ asset('bundles/template/js/ckeditor/config.js') }}"></script>
        <script type="text/javascript" src="{{ asset('bundles/template/js/ckeditor/styles.js') }}"></script>
        <script type="text/javascript" src="{{ asset('bundles/template/js/ckeditor/divarea/plugin.js') }}"></script>
        <script type="text/javascript" src="{{ asset('bundles/template/js/ckeditor/adapters/jquery.js') }}"></script>
        
        <script type="text/javascript" src="{{ asset('bundles/template/js/common-scripts.js') }}"></script>
        <script type="text/javascript" src="{{ asset('bundles/template/js/sparkline-chart.js') }}"></script>
        
        <script type="text/javascript" src="{{ asset('bundles/template/js/custom/custom.js') }}"></script>
		{% endblock %}
		
		 
    </body>
</html>
